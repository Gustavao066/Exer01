import java.util.ArrayList;
import java.util.List;

public class Exer01
{
    public static void main(String[] args) {
        List<Integer> listanumeros = new ArrayList<>();

        listanumeros.add(1);
        listanumeros.add(2);
        listanumeros.add(3);
        listanumeros.add(4);
        listanumeros.add(5);
        System.out.println(listanumeros);
        listanumeros.sort(null);
    }
}
